#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int isprime(int num){
for(int i =2;i*i<num;i++){
        if(num%i==0){
           return 0;
            
        }
        else {
            return 1;
        }
    }
}
int main (){
    int arr[7]={8,2,3,4,5,7,15};
    for(int i =0;i<7;i++){
        if(isprime(arr[i])){
            printf("%d",arr[i]);
        }
    }
    return 0;
}
